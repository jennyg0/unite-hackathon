# 1inch API Key (required) - Server-side only, NOT exposed to client
ONEINCH_API_KEY=your_1inch_api_key_here

# Private key for contract deployment (required for deployment)
PRIVATE_KEY=your_private_key_here

# Deployed contract address (will be set after deployment)
NEXT_PUBLIC_AUTOMATED_DEPOSITS_ADDRESS=

# Privy API Key (should already be set)
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id